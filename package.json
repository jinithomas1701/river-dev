{
  "name": "river-web",
  "version": "1.0.0",
  "description": "river-web",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "buildunix": "webpack -d --mode development && cp ./src/index.html ./dist/index.html && cp ./src/properties.json ./dist/properties.json && cp -rf ./src/resources ./dist/ && webpack-dev-server --content-base dist/ --inline --hot --history-api-fallback",
    "build": "webpack -d --mode development && copy .\\src\\index.html .\\dist\\index.html && copy .\\src\\properties.json .\\dist\\properties.json && xcopy .\\src\\resources .\\dist\\resources && webpack-dev-server --content-base dist/ --inline --hot --history-api-fallback",
    "build:prod": "webpack -p --mode production && copy .\\src\\index.html .\\dist\\index.html && copy .\\src\\properties.json .\\dist\\properties.json && xcopy .\\src\\resources .\\dist\\resources /s /e /h",
    "build:live": "NODE_ENV=production webpack --mode production && cp ./src/index.html ./dist/index.html && cp ./src/properties.json ./dist/properties.json && cp -rf ./src/resources ./dist/",
    "build:beta": "NODE_ENV=beta webpack --mode production && cp ./src/index.html ./dist/index.html && cp ./src/properties.json ./dist/properties.json && cp -rf ./src/resources ./dist/",
    "build:jenkin": "NODE_ENV=staging webpack --mode production && cp ./src/index.html ./dist/index.html && cp ./src/properties.json ./dist/properties.json && cp -rf ./src/resources ./dist/"
  },
  "keywords": [
    "reactjs"
  ],
  "author": "Club Dev<club.dev@litmus7.com>",
  "license": "MIT",
